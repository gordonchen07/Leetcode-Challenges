    "def reconstructQueue(people):\n",
    "    people = sorted(people, key = lambda x: (-x[0], x[1]))\n",
    "    ans = []\n",
    "    for p in people:\n",
    "        ans.insert(p[1],p)\n",
    "    return ans"
   ]
