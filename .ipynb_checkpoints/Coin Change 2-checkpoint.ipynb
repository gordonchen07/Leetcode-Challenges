    "def change(amount, coins):\n",
    "        dp = [0] * (amount + 1)\n",
    "        dp[0] = 1\n",
    "        \n",
    "        for i in coins:\n",
    "            for j in range(i, amount + 1):\n",
    "                dp[j] += dp[j - i]\n",
    "        return dp[amount]"
   ]
  },
